
<div class="row">
      <div class="col-md-4">

<h2>Log in</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name), :html => { :class => "form-horizontal"}) do |f| %>

  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "off" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

  <div class="actions">
    <%= f.submit "Log in" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>

</div>

<div class="col-md-8">
<h2>Welcome to MOVEit! </h2>
<p> Sign in to log new entries and support your network of fellow movers.  </p>
</div>
</div>
